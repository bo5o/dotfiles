{
  "mcpServers": {
    "context7": {
      "args": [
        "exec",
        "npm:@upstash/context7-mcp",
        "--",
        "context7-mcp"
      ],
      "autoApprove": [
        "get-library-docs",
        "resolve-library-id"
      ],
      "command": "mise"
    },
    "git": {
      "args": [
        "exec",
        "pipx:mcp-server-git",
        "--",
        "mcp-server-git"
      ],
      "command": "mise",
      "disabled": true
    },
    "sequentialthinking": {
      "args": [
        "exec",
        "npm:@modelcontextprotocol/server-sequential-thinking",
        "--",
        "mcp-server-sequential-thinking"
      ],
      "autoApprove": [
        "sequentialthinking"
      ],
      "command": "mise"
    },
    "vectorcode": {
      "args": [
        "exec",
        "pipx:vectorcode",
        "--",
        "vectorcode-mcp-server"
      ],
      "autoApprove": [
        "files_ls",
        "ls",
        "query"
      ],
      "command": "mise"
    }
  }
}
